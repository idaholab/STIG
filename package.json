{
    "name": "stig",
    "productName": "STIG",
    "title": "STIG",
    "description": "A tool for creating, managing, visualizing and querying structured threat intelligence.",
    "version": "2.0.1",
    "main": "src/index.ts",
    "author": {
        "name": "Jed Haile",
        "email": "jed.haile@inl.gov"
    },
    "contributors": [
        "Justin Cox",
        "Greer Galloway",
        "Michael Madsen",
        "Bryce McClurg",
        "Zachary Priest"
    ],
    "license": "SEE LICENSE IN LICENSE.txt",
    "repository": {
        "type": "git",
        "url": "https://github.com/idaholab/STIG"
    },
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint src/ --color",
        "build": "webpack",
        "start:dev": "concurrently -k \"webpack serve\" \"NODE_ENV=development nodemon index.ts\""
    },
    "nodemonConfig": {
        "ignore": [
            "src/*"
        ]
    },
    "babel": {
        "sourceMaps": "inline",
        "env": {},
        "presets": [
            [
                "env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ],
        "plugins": [
            "transform-runtime",
            "syntax-async-functions"
        ]
    },
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "zip"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {
                "packageManager": "yarn"
            },
            "electronWinstallerConfig": {
                "name": "STIG"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "STIG"
            }
        }
    },
    "resolutions": {
        "lodash": "^4.17.19",
        "elliptic": "^6.5.4",
