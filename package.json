        "minimist": "^1.2.3",
        "cryptiles": "^4.1.2",
        "yargs-parser": "^13.1.2",
        "kind-of": "^6.0.3",
        "acorn": "^7.1.1",
        "marked": "^0.7.0",
        "tar": "^4.4.2",
        "underscore": "^1.13.1",
        "decompress-zip": "^0.3.2",
        "node-fetch": "^2.6.1",
        "dot-prop": "^5.1.1",
