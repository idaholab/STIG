{
    "name": "stig",
    "productName": "STIG",
    "title": "STIG",
    "description": "A tool for creating, managing, visualizing and querying structured threat intelligence.",
    "version": "2.0.1",
    "main": "src/index.ts",
    "author": {
        "name": "Jed Haile",
        "email": "jed.haile@inl.gov"
    },
    "contributors": [
        "Justin Cox",
        "Greer Galloway",
        "Michael Madsen",
        "Bryce McClurg",
        "Zachary Priest"
    ],
    "license": "SEE LICENSE IN LICENSE.txt",
    "repository": {
        "type": "git",
        "url": "https://github.com/idaholab/STIG"
    },
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint src/ --color",
        "build": "webpack"
    },
    "babel": {
        "sourceMaps": "inline",
        "env": {},
        "presets": [
            [
                "env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ],
        "plugins": [
            "transform-runtime",
            "syntax-async-functions"
        ]
    },
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "zip"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {
                "packageManager": "yarn"
            },
            "electronWinstallerConfig": {
                "name": "STIG"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "STIG"
            }
        }
    },
    "resolutions": {},
    "dependencies": {
        "@types/cytoscape": "^3.19.0",
        "@types/jquery": "^3.5.8",
        "cytoscape": "^3.20.0",
        "cytoscape-cola": "^2.5.0",
        "cytoscape-cose-bilkent": "^4.1.0",
        "cytoscape-dagre": "^2.3.2",
        "cytoscape-edgehandles": "^4.0.1",
        "cytoscape-euler": "^1.2.2",
        "cytoscape-klay": "^3.1.4",
        "cytoscape-ngraph.forcelayout": "^1.3.9",
        "cytoscape-spread": "^3.0.0",
        "cytoscape-view-utilities": "^6.0.0",
        "jquery": "^3.6.0",
        "moment": "^2.29.1",
        "split.js": "^1.6.4",
        "ts-loader": "^9.2.6",
        "uuid": "^8.3.2",
        "webpack": "^5.61.0",
        "webpack-cli": "^4.9.1"
    },
    "engines": {
        "yarn": ">= 1.0.0"
    },
    "devDependencies": {
        "@types/jqueryui": "^1.12.16",
        "@types/uuid": "^8.3.1",
        "jqueryui": "^1.11.1"
    }
}
