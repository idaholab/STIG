{
  "name": "ngraph.asyncforce",
  "version": "1.0.0",
  "description": "Force based graph layout with web workers",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "viva": "browserify demo/vivagraph/index.js > demo/vivagraph/bundle.js",
    "pixi": "browserify demo/ngraph.pixi/index.js > demo/ngraph.pixi/bundle.js",
    "pixel": "browserify demo/ngraph.pixel/index.js > demo/ngraph.pixel/bundle.js",
    "start": "npm run pixi && npm run pixel && npm run viva"
  },
  "keywords": [
    "ngraph",
    "force",
    "layout",
    "web",
    "workers"
  ],
  "author": "Andrei Kashcha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anvaka/ngraph.asyncforce"
  },
  "devDependencies": {
    "browserify": "^11.1.0",
    "ngraph.generators": "0.0.16",
    "ngraph.pixel": "^0.1.14",
    "ngraph.pixi": "0.0.3",
    "query-string": "^2.4.1",
    "vivagraphjs": "^0.8.1"
  },
  "dependencies": {
    "ngraph.events": "0.0.3",
    "ngraph.forcelayout3d": "0.0.13",
    "ngraph.fromjson": "^0.1.8",
    "ngraph.tojson": "^0.1.4",
    "webworkify": "^1.0.2"
  }
}
